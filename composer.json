{
  "name": "dropcat/dropcat",
  "description": "delivery tool for web sites",
  "type": "drupal-deploy-tool",
  "license": "GPL-2.0-or-later",
  "authors": [
    {
        "name": "BOFH of digitalist group sweden, with friends",
        "role": "cat owners"
    }
   ],
  "config": {
    "platform": {
      "php": "7.2.5"
    }
  },
  "require": {
    "php": ">=7.2",
    "symfony/console": "^5",
    "symfony/process": "^5",
    "phpseclib/phpseclib": "^2.0",
    "symfony/config": "^5",
    "symfony/dependency-injection": "^5",
    "symfony/event-dispatcher": "^5",
    "pear/archive_tar": "^1.4",
    "padraic/phar-updater": "^1.0",
    "wkse/jenkins-php-api": "^0.1.13",
    "ocramius/proxy-manager": "^2.2",
    "symfony/proxy-manager-bridge": "^5",
    "symfony/yaml": "^5",
    "symfony/filesystem": "^5",
    "symfony/swiftmailer-bundle": "^3.4",
    "t4web/composer-lock-parser": "^1.1",
    "guzzlehttp/guzzle": "^6.5",
    "composer/xdebug-handler": "^1.4",
    "webflo/drupal-finder": "^1.2",
    "symfony/finder": "^5.0"
  },
  "bin": [
    "app/dropcat"
  ],
  "autoload": {
    "psr-4": {
      "": "src/"
    }
  },

  "require-dev": {
    "phpunit/phpunit": "^8.5",
    "dealerdirect/phpcodesniffer-composer-installer": "^0.5.0",
    "roave/security-advisories": "dev-master"
  }
}
